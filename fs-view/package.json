{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.0.2",
    "@ant-design/pro-descriptions": "^1.0.1",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.0.1",
    "@umijs/preset-react": "1.x",
    "antd": "^4.0.0",
    "dva": "^2.4.0",
    "moment": "^2.22.2",
    "umi": "^3.4.11",
    "umi-request": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-ui": "^2.2.9",
    "@umijs/test": "^3.4.11",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "react": "17.x",
    "react-dom": "17.x",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
